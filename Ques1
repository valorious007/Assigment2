#include <stdio.h>

int main()
{
   int num1, num2;
   
   printf("Enter 1st Num.:  ");
   scanf("%d0\n", &num1);
   printf("Enter 2nd Num.:  ");
   scanf("%d0\n", &num2);

    if (num1>num2)
    {
        printf("%d\n", num1);
    }
    else
    {
        printf("%d\n", num2);
    } 
    return 0;
}
